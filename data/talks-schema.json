{
  "$id": "https://ruslan.ibragimov.by/data/talks-schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Definition of Books data file.",
  "type": "array",
  "items": {
    "properties": {
      "year": {
        "type": "number"
      },
      "talks": {
        "type": "array",
        "items": {
          "properties": {
            "title": {
              "type": "string"
            },
            "link": {
              "type": "string"
            },
            "date": {
              "type": "string"
            },
            "description": {
              "type": "string"
            },
            "slides": {
              "type": "string"
            },
            "video": {
              "type": "string"
            }
          },
          "required": [
            "title",
            "link",
            "date"
          ]
        }
      }
    }
  }
}

